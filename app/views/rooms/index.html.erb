<table>
  <thead>
    <tr>
      <th>Floor</th>
      <th>Room num</th>
      <th>Room type</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.floor %></td>
        <td><%= room.room_num %></td>
        <td><%= room.room_type %></td>
        <td><%= room.available %></td>
        <td><%= room.price %></td>
        <td><%= link_to 'Show', room %></td>
        <td><%= link_to 'Edit', edit_room_path(room) %></td>
        <td><%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

